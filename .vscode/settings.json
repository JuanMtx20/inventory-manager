{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.options": {
    "extensions": [
      ".js"
    ]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "css.validate": true,
  "css.lint.emptyRules": "warning",
  "css.lint.unknownAtRules": "error",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2565ac",
    "activityBar.background": "#2565ac",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#e37cad",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#2565ac",
    "statusBar.background": "#1c4c82",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2565ac",
    "statusBarItem.remoteBackground": "#1c4c82",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1c4c82",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1c4c8299",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1C4C82",
}